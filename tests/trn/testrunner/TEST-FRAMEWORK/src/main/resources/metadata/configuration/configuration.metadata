CONFIGURATION{
	LANG en_EN;
	LANG es_ES;

	DATATYPE VARCHAR SQLNATIVETYPE VARCHAR WITHLENGTH JAVATYPE String ;
	DATATYPE INTEGER SQLNATIVETYPE INT JAVATYPE Integer ;
	DATATYPE DATETIME SQLNATIVETYPE DATETIME JAVATYPE  java.util.Date;
	DATATYPE NUMERIC SQLNATIVETYPE NUMERIC WITHPRECISSIONANDSCALE JAVATYPE java.math.BigDecimal;
	DATATYPE MONEY SUBTYPEOF NUMERIC;
	
	COLUMNSTEREOTYPE DESCRIPTION;
	
	PATTERN Enumeration{
		shortCode VARCHAR(16)
		description VARCHAR(32)
	}

	PATTERN AutoHistory{
		PK revisionId INTEGER
		startDate DATETIME NOTNULL
		endDate DATETIME NOTNULL DEFAULT "'01-Jan-2099'"
	}
	

	DEFAULT_PRIMARY_KEY_GENERATION_STRATEGY SEQUENCE
}
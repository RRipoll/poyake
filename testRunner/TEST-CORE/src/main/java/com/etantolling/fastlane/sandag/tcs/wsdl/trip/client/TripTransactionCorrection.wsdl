					<xsd:complexType>
						<xsd:sequence>
							<xsd:element minOccurs="1" maxOccurs="1" nillable="false" name="tripSequenceNumber" type="xsd:long"/>
							<xsd:element minOccurs="1" maxOccurs="1" nillable="false" name="oldFareAmount" type="cmn:OldFareAmountType"/>

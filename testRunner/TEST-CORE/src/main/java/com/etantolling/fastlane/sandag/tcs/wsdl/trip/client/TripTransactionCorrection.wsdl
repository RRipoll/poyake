	</wsdl:message>
	<wsdl:message name="sendTripTransactionCorrectionResponse">
		<wsdl:part name="response" element="tns:ReturnCode"/>
	</wsdl:message>
	<!-- End Messages -->
	
	<!-- Begin PortType-->
	<wsdl:portType name="TripTransactionCorrectionInterfacePortType">
		<wsdl:operation name="sendTripTransactionCorrection">
			<wsdl:input name="sendTripTransactionCorrectionRequest" message="tns:sendTripTransactionCorrectionRequest"/>
			<wsdl:output name="sendTripTransactionCorrectionResponse" message="tns:sendTripTransactionCorrectionResponse"/>
		</wsdl:operation>
	</wsdl:portType>
	<!-- End PortType-->
	
	<!-- Begin Binding -->

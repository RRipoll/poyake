					</xsd:restriction>
				</xsd:simpleType>
				
				<xsd:element name="ReturnCode">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Code" type="xsd:nonNegativeInteger" />
							<xsd:element minOccurs="0" nillable="true" name="Description" type="xsd:string" />
							<xsd:element minOccurs="0" nillable="true" name="SeverityCode" type="xsd:string" />
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:schema>
	</wsdl:types>
	

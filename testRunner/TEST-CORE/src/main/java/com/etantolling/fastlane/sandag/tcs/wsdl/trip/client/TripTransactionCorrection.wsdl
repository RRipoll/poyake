			<wsdl:input name="sendTripTransactionCorrectionRequest">
				<soap:body use="literal"/>
			</wsdl:input>
			<wsdl:output name="sendTripTransactionCorrectionResponse">
				<soap:body use="literal"/>
			</wsdl:output>

					</xsd:complexType>
				</xsd:element>
				
				<xsd:simpleType name="OldFareAmountType">
					<xsd:restriction base="xsd:decimal">
						<xsd:minInclusive value="0.00" />
						<xsd:maxInclusive value="99999.99" />
					</xsd:restriction>
				</xsd:simpleType>
				
				<xsd:simpleType name="NewFareAmountType">
					<xsd:restriction base="xsd:decimal">
						<xsd:minInclusive value="0.00" />

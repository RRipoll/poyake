TABLE ENTITY payment.EnuPaymentType PATTERN Enumeration{
	PK paymentTypeId INT
	shortCode VARCHAR ( 16 )
	description VARCHAR ( 32 ) STEREOTYPES DESCRIPTION
	paymentTypeClassId INT NOTNULL FKTO payment.EnuPaymentTypeClass
	
	ENUMETADATA{
		1, VISA, 'Visa', 1;
		2, MCARD,'MasterCard', 1;
		3, AMEX, "American Express", 1;
		4, DISC, "Discovery", 1;
		5, CASH, 'Cash', 2;
		6, DIRECTDEBIT, 'Direct Debit', 3;
		7, CHECK, 'Check', 4;
	}
	
	LABELS{
		paymentTypeId {en_EN 'Type' es_ES 'Tipo'}
	}
}
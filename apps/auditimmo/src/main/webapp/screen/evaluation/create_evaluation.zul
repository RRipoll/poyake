<zk xmlns="xhtml" xmlns:z="zul" xmlns:w="http://www.zkoss.org/2005/zk/client">
	<section class="content m-0">
			<div class="card m-0">
				<div class="header bg-light">
					<h2>Create Evaluation</h2>
				</div>
				<div class="body p-3 pt-5">

					<!-- Folder -->
					<div class="row clearfix">
						<div class="col-2 form-control-label">
                             <label class="font-bold">Folder</label>
                        </div>					
						<div class="col-5">
							<div class="form-group">
								<z:div class="form-line">
									<z:mt-spanobjectdescription id="FOLDER_NAME" field="EVALUATION.FOLDERID">Folder</z:mt-spanobjectdescription>
								</z:div>
							</div>
						</div>
						<div class="col-1 form-control-label">
                             <button class="btn btn-secondary" id="SEARCH_FOLDER">Search</button>
                        </div>					
					</div>
					
					<!-- Evaluator -->
					<div class="row clearfix">
						<div class="col-2 form-control-label">
                             <label class="font-bold">Evaluator</label>
                        </div>					
						<div class="col-5">
							<div class="form-group">
								<z:div class="form-line">
									<z:mt-spanobjectdescription id="EVALUATOR_NAME" field="EVALUATION.EVALUATORID">Evaluator</z:mt-spanobjectdescription>
								</z:div>
							</div>
						</div>
						<div class="col-1 form-control-label">
                             <button class="btn btn-secondary" id="SEARCH_EVALUATOR">Search</button>
                        </div>					
					</div>

					<!-- Applicant -->
					<div class="row clearfix">
						<div class="col-2 form-control-label">
                             <label class="font-bold">Applicant</label>
                        </div>					
						<div class="col-5">
							<div class="form-group">
								<z:div class="form-line">
									<z:mt-spanobjectdescription id="APPLICANT_NAME" field="EVALUATION.APPLICANTID">Applicant</z:mt-spanobjectdescription>
								</z:div>
							</div>
						</div>
						<div class="col-1 form-control-label">
                             <button class="btn btn-secondary" id="SEARCH_APPLICANT">Search</button>
                        </div>					
						<div class="col-1 form-control-label">
                             <button class="btn btn-secondary" id="CREATE_APPLICANT">Create</button>
                        </div>					
					</div>

					<!-- Applicant -->
					<div class="row clearfix">
						<div class="col-2 form-control-label">
                             <label class="font-bold">Mandate</label>
                        </div>					
                        <z:radiogroup id="mandate"/>
						<div class="col-2">
							<z:mt-radio radiogroup="mandate" field="EVALUATION.MANDATETYPEID" shortcode="EXISTING" />
						</div>
						<div class="col-2">
							<z:mt-radio radiogroup="mandate" field="EVALUATION.MANDATETYPEID" shortcode="SPECIFIC" />
						</div>
					</div>

					<!-- Habitability -->
					<div class="row clearfix">
						<div class="col-2 form-control-label">
                             <label class="font-bold">Habitability</label>
                        </div>					
                        <z:radiogroup id="HABITABILITY"/>
						<div class="col-2">
							<z:mt-radio radiogroup="HABITABILITY" field="EVALUATION.HABITABILITYID" shortcode="EXTERIOR"/>
						</div>
						<div class="col-2">
							<z:mt-radio radiogroup="HABITABILITY" field="EVALUATION.HABITABILITYID" shortcode="BOTH"/>
						</div>
					</div>

					<!-- Sanitary State -->
					<div class="row clearfix">
						<div class="col-2 form-control-label">
                             <label class="font-bold">Sanitary State</label>
                        </div>					
                        <z:radiogroup id="sanitary"/>
						<div class="col-2">
							<z:mt-radio radiogroup="sanitary" field="EVALUATION.SANITARYSTATEID" shortcode="EXTERIOR" />
						</div>
						<div class="col-2">
							<z:mt-radio radiogroup="sanitary" field="EVALUATION.SANITARYSTATEID" shortcode="BOTH" />
						</div>
					</div>

					<!-- Sanitary State -->
					<div class="row clearfix">
						<div class="col-2 form-control-label">
                             <label class="font-bold">Date / Time</label>
                        </div>					
						<div class="col-3">
							<div class="form-group">
								<z:div class="form-line">
									<z:datebox />
								</z:div>
							</div>
						</div>
						<div class="col-2">
							<div class="form-group">
								<z:div class="form-line">
									<z:timebox />
								</z:div>
							</div>
						</div>
					</div>

					<button id="CANCEL_BUTTON" class="btn btn-danger ml-2 waves-effect pull-right">Cancel</button>
					<button id="SAVE_BUTTON" class="btn btn-primary ml-2 waves-effect pull-right">Save</button>
					<button id="GO_TO_SANITARY" class="btn btn-primary ml-2 waves-effect pull-right">Go To Sanitary State</button>
					<button id="GO_TO_HABITABILITY" class="btn btn-primary ml-2 waves-effect pull-right">Go To Habitability</button>
				</div>
			</div>
	</section>
</zk>
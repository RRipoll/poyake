TABLE ENTITY eva.Intervenant{
	PK intervenantId INT
	contactId INT NOTNULL FKTO crm.ContactPk
	inputUserId INT FKTO audit.InputUser
	intervenantCategoryId INT NOTNULL FKTO eva.EnuIntervenantCategory
	networkId INT NOTNULL FKTO eva.EnuNetwork
	active BIT NOTNULL DEFAULT "1"

	METADATA{
		1, 1, 1, 1, 1, 1;
		2, 2, 2, 1, 1, 1;
	}
		
	LABELS{
		intervenantCategoryId {SHORTLABEL en_EN 'Category'}
		networkId {SHORTLABEL en_EN 'Network'}
	}
	
}
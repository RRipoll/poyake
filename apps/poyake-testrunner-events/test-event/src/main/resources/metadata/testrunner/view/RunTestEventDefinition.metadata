VIEW  ENTITY testrunner.RunTestEventDefinition  {
	PK eventDefinitionUuid 	UUID  			FKTO testrunner.EventDefinition
    eventTypeId 			UUID 			NOTNULL SAMEAS testrunner.EventDefinition.eventTypeId 
    eventName 				VARCHAR(64) 	NOTNULL STEREOTYPES DESCRIPTION 
    description 			VARCHAR(255) 	SAMEAS testrunner.EventDefinition.description
	selected 				EVENTSELECTED 	DEFAULT "1"
    testId 					UUID  			FKTO testrunner.Test
	repetitionNumber  		INT  			NOTNULL DEFAULT '1'
	}


    <content-type
          base-type="org.eclipse.core.runtime.text"
          file-extensions="poyakegraph"
          id="com.jsantos.metadata.plugin.ui.grapheditor.poyakegraph"
          name="Poyake Graph">
    </content-type>
 </extension>

<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
	<extension point="org.eclipse.ui.editors">
		<editor
			class="com.jsantos.metadata.plugin.ui.MetaDslExecutableExtensionFactory:org.eclipse.xtext.ui.editor.XtextEditor"
			contributorClass="org.eclipse.ui.editors.text.TextEditorActionContributor"
			default="true"
			extensions="metadata"
			id="com.jsantos.metadata.plugin.MetaDsl"
			name="MetaDsl Editor">
		</editor>
	    <editor id="com.jsantos.metadata.plugin.ui.poyakegrapheditor"
	         name="Poyake Graph Editor"
	 		 extensions="poyakegraph" 
	 		 class="com.jsantos.metadata.plugin.ui.grapheditor.GraphEditorPart" 
	 		 contributorClass="com.jsantos.metadata.plugin.ui.grapheditor.GraphEditorContributor"> 
	    </editor>
	</extension>
	<extension
		point="org.eclipse.ui.handlers">
		<handler
			class="com.jsantos.metadata.plugin.ui.MetaDslExecutableExtensionFactory:org.eclipse.xtext.ui.editor.hyperlinking.OpenDeclarationHandler"
			commandId="org.eclipse.xtext.ui.editor.hyperlinking.OpenDeclaration">
			<activeWhen>
				<reference
					definitionId="com.jsantos.metadata.plugin.MetaDsl.Editor.opened">
				</reference>
			</activeWhen>
		</handler>
		<handler
			class="com.jsantos.metadata.plugin.ui.MetaDslExecutableExtensionFactory:org.eclipse.xtext.ui.editor.handler.ValidateActionHandler"
			commandId="com.jsantos.metadata.plugin.MetaDsl.validate">
		<activeWhen>
			<reference
					definitionId="com.jsantos.metadata.plugin.MetaDsl.Editor.opened">
			</reference>
		</activeWhen>
		</handler>
		<!-- copy qualified name -->
		<handler
			class="com.jsantos.metadata.plugin.ui.MetaDslExecutableExtensionFactory:org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedNameHandler"
			commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName">
			<activeWhen>
				<reference definitionId="com.jsantos.metadata.plugin.MetaDsl.Editor.opened" />
			</activeWhen>
		</handler>
		<handler
			class="com.jsantos.metadata.plugin.ui.MetaDslExecutableExtensionFactory:org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedNameHandler"
			commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedName">
			<activeWhen>
				<and>
					<reference definitionId="com.jsantos.metadata.plugin.MetaDsl.XtextEditor.opened" />
					<iterate>
						<adapt type="org.eclipse.xtext.ui.editor.outline.IOutlineNode" />
					</iterate>
				</and>
			</activeWhen>
		</handler>
  <handler
        class="com.jsantos.metadata.plugin.ui.handlers.DBResetHandler"
        commandId="com.jsantos.metadata.plugin.ui.commands.ResetDatabaseCommand">
  </handler>
  <handler
        class="com.jsantos.metadata.plugin.ui.handlers.TestDatabaseConnectionHandler"
        commandId="com.jsantos.metadata.plugin.ui.commands.TestDatabaseConnection">
  </handler>
  <handler
        class="com.jsantos.metadata.plugin.ui.handlers.ParseSQLHandler"
        commandId="com.jsantos.metadata.plugin.ui.commands.ParseSQLCommand">
  </handler>
  <handler
        class="com.jsantos.metadata.plugin.ui.handlers.TestHandler"
        commandId="com.jsantos.metadata.plugin.ui.commands.TestCommand">
  </handler>
  <handler
        class="com.jsantos.metadata.plugin.ui.handlers.RebuildSourcesHandler"
        commandId="com.jsantos.metadata.plugin.ui.commands.RebuildSourcesCommand">
  </handler>
  <handler
        class="com.jsantos.metadata.plugin.ui.handlers.ReverseEngineerHandler"
        commandId="com.jsantos.metadata.plugin.ui.commands.ReverseEngineerCommand">
  </handler>
  
	</extension>
	<extension point="org.eclipse.core.expressions.definitions">
		<definition id="com.jsantos.metadata.plugin.MetaDsl.Editor.opened">
			<and>
				<reference definitionId="isActiveEditorAnInstanceOfXtextEditor"/>
				<with variable="activeEditor">
					<test property="org.eclipse.xtext.ui.editor.XtextEditor.languageName"
						value="com.jsantos.metadata.plugin.MetaDsl"
						forcePluginActivation="true"/>
				</with>
			</and>
		</definition>
		<definition id="com.jsantos.metadata.plugin.MetaDsl.XtextEditor.opened">
			<and>
				<reference definitionId="isXtextEditorActive"/>
				<with variable="activeEditor">
					<test property="org.eclipse.xtext.ui.editor.XtextEditor.languageName"
						value="com.jsantos.metadata.plugin.MetaDsl"
						forcePluginActivation="true"/>
				</with>
			</and>
		</definition>
	</extension>
	<extension
			point="org.eclipse.ui.preferencePages">
		<page
			class="com.jsantos.metadata.plugin.ui.MetaDslExecutableExtensionFactory:org.eclipse.xtext.ui.editor.preferences.LanguageRootPreferencePage"
			id="com.jsantos.metadata.plugin.MetaDsl"
			name="MetaDsl">
			<keywordReference id="com.jsantos.metadata.plugin.ui.keyword_MetaDsl"/>
		</page>
		<page
			category="com.jsantos.metadata.plugin.MetaDsl"
			class="com.jsantos.metadata.plugin.ui.MetaDslExecutableExtensionFactory:org.eclipse.xtext.ui.editor.syntaxcoloring.SyntaxColoringPreferencePage"
			id="com.jsantos.metadata.plugin.MetaDsl.coloring"
			name="Syntax Coloring">
			<keywordReference id="com.jsantos.metadata.plugin.ui.keyword_MetaDsl"/>
		</page>
		<page
			category="com.jsantos.metadata.plugin.MetaDsl"
			class="com.jsantos.metadata.plugin.ui.MetaDslExecutableExtensionFactory:org.eclipse.xtext.ui.editor.templates.XtextTemplatePreferencePage"
			id="com.jsantos.metadata.plugin.MetaDsl.templates"
			name="Templates">
			<keywordReference id="com.jsantos.metadata.plugin.ui.keyword_MetaDsl"/>
		</page>
		<page
			category="com.jsantos.metadata.plugin.MetaDsl"
			class="com.jsantos.metadata.plugin.ui.preferences.MetaDslPreferencesPage"
			id="com.jsantos.metadata.plugin.MetaDsl.database"
			name="Database Connection">
			<keywordReference id="com.jsantos.metadata.plugin.ui.keyword_MetaDsl"/>
		</page>
	</extension>
	<extension
			point="org.eclipse.ui.propertyPages">
		<page
			class="com.jsantos.metadata.plugin.ui.MetaDslExecutableExtensionFactory:org.eclipse.xtext.ui.editor.preferences.LanguageRootPreferencePage"
			id="com.jsantos.metadata.plugin.MetaDsl"
			name="MetaDsl">
			<keywordReference id="com.jsantos.metadata.plugin.ui.keyword_MetaDsl"/>
			<enabledWhen>
				<adapt type="org.eclipse.core.resources.IProject"/>
			</enabledWhen>
			<filter name="projectNature" value="org.eclipse.xtext.ui.shared.xtextNature"/>
		</page>
	</extension>
	<extension
		point="org.eclipse.ui.keywords">
		<keyword
			id="com.jsantos.metadata.plugin.ui.keyword_MetaDsl"
			label="MetaDsl"/>
	</extension>
	<extension

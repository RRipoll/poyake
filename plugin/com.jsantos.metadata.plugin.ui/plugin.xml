			class="com.jsantos.metadata.plugin.ui.MetaDslExecutableExtensionFactory:org.eclipse.xtext.ui.editor.quickfix.MarkerResolutionGenerator"
			markerType="com.jsantos.metadata.plugin.ui.metadsl.check.fast">

 <command
       categoryId="com.jsantos.metadata.plugin.ui.commands.category"
       id="com.jsantos.metadata.plugin.ui.commands.ResetDatabaseCommand"
       name="Reset Database">
 </command>
 <command
       categoryId="com.jsantos.metadata.plugin.ui.commands.category"
       id="com.jsantos.metadata.plugin.ui.commands.TestDatabaseConnection"
       name="Test Database Connection">
 </command>
 <command
       categoryId="com.jsantos.metadata.plugin.ui.commands.category"
       id="com.jsantos.metadata.plugin.ui.commands.ParseSQLCommand"
       name="Parse SQL">
 </command>
 <command
       categoryId="com.jsantos.metadata.plugin.ui.commands.category"
       id="com.jsantos.metadata.plugin.ui.commands.TestCommand"
       name="Test Code Entry Point">
 </command>
 <command
       categoryId="com.jsantos.metadata.plugin.ui.commands.category"
       id="com.jsantos.metadata.plugin.ui.commands.RebuildSourcesCommand"

			type="metadata">
		</parser>
	</extension>
	<extension point="org.eclipse.xtext.extension_resourceServiceProvider">
		<resourceServiceProvider
			class="com.jsantos.metadata.plugin.ui.MetaDslExecutableExtensionFactory:org.eclipse.xtext.ui.resource.IResourceUIServiceProvider"
			uriExtension="metadata">
		</resourceServiceProvider>
	</extension>
	<!-- marker definitions for com.jsantos.metadata.plugin.MetaDsl -->
